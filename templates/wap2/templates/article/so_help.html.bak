<ion-view hide-nav-bar="true" >
  <div class="bar bar-header bar-stable ">
    <a href="#/tab/article" class="button icon ion-ios-arrow-back button-clear head_button1">返回</a>
    <h1 class="title">搜索下单指引</h1>
  </div>
  <ion-content class="padding_head">

    <div class="ibody">
      <div class="i_tryDetail3">
        <div class="part">
          <table width="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>

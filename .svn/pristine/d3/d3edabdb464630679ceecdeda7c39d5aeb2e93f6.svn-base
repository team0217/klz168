<script type="text/javascript">
	function merAnn(){
		var liNum = $(".position li").length;
		var _top = $(".position li").height();
		var top = 0;
		var i = 1;
		setInterval(function(){
			if(i == liNum){
				$(".position ul").animate({top:0});
				i = 1;
				top = 0;
			}
			else{
				top -= _top;
				$(".position ul").animate({top:top + "px"});
				i++;
			}
		},3000);

		$(".next").click(function(){
			if(i == liNum){
				i = 1;
				top = 0;
				$(".position ul").animate({top:0});
			}
			else{
				top -= _top;
				$(".position ul").animate({top:top + "px"});
				i++;
			}
		});
		
		$(".pre").click(function(){
			if(i == 1){
				i = 1;
				$(".position ul").css({top:0});
			}
			else{
				top += _top;
				$(".position ul").animate({top:top + "px"});
				i--;
			}
		});
	}
</script>
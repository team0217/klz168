<ion-view hide-nav-bar="true" >
  <div class="bar bar-header  bar-stable ">
    <a href="#/tab/user" class="button icon ion-ios-arrow-back button-clear "></a>
    <h1 class="title">我的积分兑换</h1>
  </div>
  <div class="bar bar-subheader">
    <div class="button-bar sub_header_list" id="sub_header_list">
      <a class="button button-clear" ng-click="jifen_order_changeTab(0)">审核中</a>
      <a class="button button-clear sub_button_select" ng-click="jifen_order_changeTab(1)">已完成</a>
    </div>
  </div>

  <ion-content class="has-subheader"  style="background-color:#F8F8F8">
    <div ng-if="shop_log_showloading" style="margin-top:60px;text-align:center">
      <ion-spinner icon="ios" style="height: 60px; width: 60px;;"></ion-spinner>
    </div>

    <ion-list class="list padding-top" ng-if="shop_log_showloading==false">

     <div class="muyou" ng-if="!shop_log ">
      <img ng-src="img/muyou.png">
      <p> <span>亲 </span>您当前木有对应下的兑换商品</p>
      <a href="#/tab/jifen"  class="button  button-assertive button-outline ">去用积分兑换商品</a>
     </div>
  
      <div class="ibody">
        <div class="i_exchangeRecord">
          <div class="s_list1">
            <div class="part" ng-repeat="d in shop_log">
              <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                  <td class="L">
                    <img ng-src="{{d.img |imgUrl}}"></td>
                  <td class="R">
                    <div class="hang1">{{d.title}}</div>
                    <div class="hang2">
                      花费积分：
                      <span>{{d.point}}</span>
                      <br>兑换时间：{{d.apply_time +'000' | date:"yyyy/MM/dd h:mm"}}</div>
                    <div class="hang3">
                      <span style="position:absolute;left:110px" >状态:{{d.status | f_jifen_status}}</span>
                      <a href="#/tab/jifen/{{d.shop_id}}">查看商品</a>
                    </div>
                  </td>
                </tr>
              </table>
            </div>
            <!--part--> </div>

        </div>
      </div>

    </ion-list>
  </ion-content>
</ion-view>
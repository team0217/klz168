/* 重置浏览器默认样式 */
body, div, ul, ol, li, dl, dt, dd, table, thead, tbody, tfoot, th, tr, td, h1, h2, h3, h4, h5, h6, font, strong, em, i, b, form, input, textarea, select, option, p, span, a, img, embed, frameset, frame, iframe{
	margin: 0;
	padding: 0;
	border: none;
	border-spacing: 0;
	border-collapse: collapse;
	list-style: none;
	text-decoration: none;
	font: 12px 'Microsoft Yahei';
	color: #636363;
	
}
/* 浮动与清除 */
.fl{
	float: left;
}

.fr{
	float: right;
}

.clear{
	clear: both;
}

/* 主体内容 */
#main{
	padding: 25px 20px 0;
}

/* 登陆信息 */
.logo_info{
	margin-bottom: 10px;
}

.logo_info span{
	color: #275d96;
}

/* 升级区域 */
.up{
	height: 28px;
	line-height: 27px;
	border: 1px solid #e0e0e0;
	padding-left: 28px;
	background: url(../images/up_bg.png) no-repeat;
}

.up span{
	color: #000000;
}

.up a{
	color: #1c98f0;
}

/* 订单信息与产品信息通用样式 */
.bg{
	background: #f1f1f1;
}

#main h1{
	height: 30px;
	padding: 0 10px;
	border-bottom: 1px solid #d4d4d4;
}

.mt26{
	margin-top: 26px;
}

.mb20{
	margin-bottom: 20px;
}


.f14{
	font-size: 14px;
}

.fw{
	font-weight: 700;
}

.c3a6ea5{
	color: #3a6ea5;
}

.f6f6f6{
	color: #f6f6f6;
}

.c000000{
	color: #000000;
}

#main dd{
	width: 32%;
	margin-right: 1.7%;
}

#main dd ul{
	border: 1px solid #d4d4d4;
}

#main .title{
	background: #f6f6f6;
}

#main li{
	height: 30px;
	border-bottom: 1px solid #d4d4d4;
	padding: 0 10px;
}

.lh28{
	line-height: 28px;
}

.ib{
	display: inline-block;
}

.bt_none{
	border-bottom: none !important;
}

/* 版权 */
#copyright{
	border-top: 1px solid #e5e5e5;
	margin-top: 110px;
	padding: 5px 0 50px 0;
	text-align: center;
}

/* 雪毅网络开发团队 */
#main .team_bg{
	background: url(../images/team_bg.jpg) no-repeat 0 center;
	padding-left: 15px;
}

.w35{
	width: 40%;
}

.mr6{
	margin-right: 6%;
}

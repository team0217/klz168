/* 多个左滚动 - 公用类 */
.leftLoop{ overflow:hidden; position:relative;   }
.leftLoop .hd{ overflow:hidden;  height:30px; }
.leftLoop .hd .prev,.leftLoop .hd .next{ display:block;  width:10px; height:10px; line-height:10px; float:right; margin-right:10px; margin-top:10px;  overflow:hidden;cursor:pointer; background:url("../images/left.png") no-repeat;}
.leftLoop .hd .next{ background:url("../images/right.png") no-repeat;}
.leftLoop .hd ul{ float:right; overflow:hidden; zoom:1; margin-top:12px;  }
.leftLoop .hd ul li{ position:relative; _display:inline; float:left;  width:7px; height:7px; overflow:hidden; background:#ccc; margin-right:10px; text-indent:-999px; cursor:pointer;-webkit-border-radius: 10px; -moz-border-radius: 10px; border-radius: 10px;}
.leftLoop .hd ul li.on{ background:#f45a4f;  }
.leftLoop .bd ul{ overflow:hidden; zoom:1; }
.leftLoop .bd ul li{ margin:0 5px; text-align:center; float:left;display:_inline;  }

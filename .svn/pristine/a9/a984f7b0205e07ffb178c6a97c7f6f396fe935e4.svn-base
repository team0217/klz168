/*最顶部*/
.site-nav:after {
    clear: both;
    content: "";
    display: table;
}
.site-nav {
    background: none repeat scroll 0 0 #f5f5f5;
    border-bottom: 1px solid #dddddd;
    width: 100%;
}
.site-nav .site-nav-bd:after {
    clear: both;
    content: "";
    display: table;
}
.site-nav .site-nav-bd {
    width: 1200px;
}
.site-nav .site-nav-bd {
    background: none repeat scroll 0 0 #f5f5f5;
    height: 35px;
    margin: 0 auto;
    width: 990px;
}
.site-nav-bd-l .menu, .site-nav .site-nav-bd .site-nav-bd-r .menu {
    float: left;
}
.site-nav-bd .menu .menu-hd a {
    color: #5c5c5c;
    float: left;
    font-size: 12px;
    margin-right: 7px;
}
.site-nav-bd .h {
    color: #f22e00 !important;
}
.site-nav-bd a {
    color: #3c3c3c;
}
.site-nav .site-nav-bd .site-nav-bd-r {
    float: right;
}
.site-nav-bd .menu .menu-hd {
    line-height: 35px;
}

.topsearch {
    width: 460px;
    height: 35px;
    margin: auto;
    background: #fff;
    border-radius: 20px;
    border: 1px solid #e2e1e1;
    z-index: 2;
    position: relative;
    zoom: 1;
    margin-top: 20px;
}
.searchshadow {
    height: 35px;
    border-radius: 20px;
    position: relative;
    zoom: 1;
    padding-left: 35px;
}
.selctsearch {
    background: url("../images/search.png") no-repeat;
    height: 21px;
    line-height: 35px;
    width: 21px;
    text-align: center;
    position: absolute;
    left: 8px;
    top: 8px;
    padding-left: 13px;
}
.topsearch .text {
    width: 75%;
    height: 30px;
    margin: 2px 0 0 0;
    background: 0;
    display: inline-block;
    border: 0;
    font-size: 14px;
    position: absolute;
}
.topsearch .button {
    border: 0;
    width: 75px;
    height: 35px;
    cursor: pointer;
    border-bottom-right-radius:20px;
    border-top-right-radius:20px;
    float: right;
    background: #fe7e00;
    color: #ffffff;
    font-size: 16px;
}


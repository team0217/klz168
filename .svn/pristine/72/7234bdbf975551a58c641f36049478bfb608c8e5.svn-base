<body>
	{include template('merchant_sign_header','member/common')}
	<div id="banner"><img src="{THEME_STYLE_PATH}style/images/shop_sign_banner.jpg" /></div>
	<div id="main">		
		<div class="ann">
			<dl>
				<dt class="fl">招商公告：</dt>
				<dd class="position fl">
					<ul>
						{pc:announce action="lists" num="3" type="2" order="listorder"}
						{loop $data $d}
						<li><a href="{U('Announce/Index/show',array('id'=>$d[announceid],'type'=>2))}">{$d['title']}</a></li>
						{/loop}
						{/pc}
					</ul>
				</dd>
				<dd class="btn fr">
					<span class="pre"></span>
					<span class="next"></span>
				</dd>
			</dl>
			<div class="clear"></div>
		</div>
		<div class="process">
			<dl>
				<dt class="f22 c3c3c3c mt40 mb20">合作流程</dt>
				<dd>
					<ul>
						<li class="sign_ico fl h38 mt45 pl57 pt5">活动报名</li>
						<li class="arrow fl"></li>
						<li class="check_ico fl h38 mt45 pl57 pt5">商品审核</li>
						<li class="arrow fl"></li>
						<li class="sch_ico fl h38 mt45 pl57 pt5">活动排期</li>
						<li class="arrow fl"></li>
						<li class="on_line_ico fl h38 mt45 pl57 pt5">上线展示</li>
					</ul>
					<div class="clear"></div>
				</dd>
			</dl>
		</div>
		<!-- 合作流程 end -->
		<!-- 商家报名 start -->
		<a name="sign_up"></a>
		<div class="sign">
			<dl>
				<dt class="f22 c3c3c3c mt40 mb20">商家报名</dt>
				<dd>
					<ul>
						{if is_activity_open('rebate') == 1}
						<li class="fl">
							<div class="rebates bg"></div>
							<p class="sign_name">{$act_name['rebate']}</p>
							<p class="exp">{$activityarr['rebate'][seller_activity_desc]}</p>
							<p class="act_time">活动时间：{$activityarr['rebate'][seller_start_time]}~{$activityarr['rebate'][seller_end_time]}</p>
							<p><a href="{U('Member/EnterActivity/detail_activity',array('mod'=>'rebate'))}" class="sign_btn">马上报名</a></p>
							<p class="look"><a href="#">查看展示位></a></p>
						</li>
						{/if}
						
						{if is_activity_open('trial') == 1}

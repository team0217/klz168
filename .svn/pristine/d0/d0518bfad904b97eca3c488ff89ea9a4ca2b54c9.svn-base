	<script type="text/javascript">
		$(document).ready(function(){
			$("#loading").ajaxStart(function(){
				$(".portrait").hide();
	            $(this).show();
	         }).ajaxComplete(function(){
	            $(this).hide();
	            $(".portrait").show();
	         });
			
			$("#image").click(function (){
				$("#file_upload").click();
			});

			$("#logo").click(function (){
				$("#file_upload").click();
			});
		});

			$("#js_submit").click(function(){


				var name = $("#store_name").val();
				var id = $("#ids").val();

				var store_address = $("#store_address").val();
				var contact_name = $("#contact_name").val();
				var contact_want = $("#contact_want").val();
				var store_qq = $("#store_qq").val();
				var industry = $("#industry").val();
                var store_logo = $("#file_url").val();
                var store_type = $("#store_type").val();

				if (name == '' ) {
					$("#name").focus();
					return false;
				}

				 if (store_address == '' ) {
				 	$("#store_address").focus();
				 	return false;
				 }
				 
				 if (contact_name == '' ) {
				 	$("#contact_name").focus();
				 	return false;
				 }
				 
				 if (store_qq == '' ) {
				 	$("#store_qq").focus();
				 	return false;
				 }
				 
				 if (industry == '' ) {
				 	$("#industry").focus();
				 	return false;
				 }
				 

				var info = {
					id:id,
					store_name:name,
					store_address:store_address,
					contact_name:contact_name,
					contact_want:contact_want,
					store_qq:store_qq,
					industry:industry,
					store_logo:store_logo,
					store_type:store_type
				}



		$.post("{U('Member/Merchant/store_info')}",{info:info},function(data){
								if (data.status == 1) {
									art.dialog({
									lock: true,
									fixed: true,
									icon: 'face-smile',
									title: '提示',
									content: data.info,
									okVal: '确定',
									ok:function() { 
										location.href='{U('Member/Merchant/complete')}';
										/*location.reload();*/
									}
								});
		                       

								}else{
									art.dialog({
									lock: true,
									fixed: true,
									icon: 'face-sad',
									title: '错误提示',
									content: data.info,
									ok: true
								});

									};
													
							},'json');
			});


		$('.js_del').click(function(){
			var id = $(this).attr('data-id');
			$.post("{U('Member/Merchant/del')}",{id:id},function(data){
				if (data.status == 1) {
						art.dialog({
						lock: true,
						fixed: true,
						icon: 'face-smile',
						title: '提示',
						content: data.info,
						okVal: '确定',
						ok:function() { 
							location.reload();
						}
					});
                   

					}else{
						art.dialog({
						lock: true,
						fixed: true,
						icon: 'face-sad',
						title: '错误提示',
						content: data.info,
						ok: true
					});

				};
													

			},'json');
		});

    $('.js_default').click(function(){
      var id = $(this).attr('data-id');
      $.post("{U('Member/Merchant/set_default')}",{id:id},function(data){
        if (data.status == 1) {
            art.dialog({
            lock: true,
            fixed: true,
            icon: 'face-smile',
            title: '提示',
            content: data.info,
            okVal: '确定',
            ok:function() { 
              location.reload();
            }
          });
                   

          }else{
            art.dialog({
            lock: true,
            fixed: true,
            icon: 'face-sad',
            title: '错误提示',
            content: data.info,
            ok: true
          });

        };
                          

      },'json');
    });


		</script>
#zc-content .user_l_c .u_tab_list .hint{ width:100%; float:left; padding-left:122px; font-size:12px; height:30px; line-height:30px; }
#zc-content .user_l_c .u_tab_list .hint.suc{ color:green; }
#zc-content .user_l_c .u_tab_list .hint.err{ color:red; }

#zc-content .user_l_c .u_tab_list li.yzm input{ width:150px; }

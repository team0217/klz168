#zc-content .user_l_c .u_tab li.u_p b{ background:url(../images/u_phone_off.png) no-repeat 6px center; }
#zc-content .user_l_c .u_tab li.active{ border-color:#FF6C00; }
#zc-content .user_l_c .u_tab li.active a{ color:#FF6C00; }
#zc-content .user_l_c .u_tab li.u_p.active b{ background:url(../images/u_phone_on.png) no-repeat 6px center; }
#zc-content .user_l_c .u_tab li.u_e.active b{ background:url(../images/u_email_on.png) no-repeat left center; }

#zc-content .user_l_c .u_tab_list{ width:580px; margin:0 auto; padding-top:15px; }
#zc-content .user_l_c .u_tab_list li{ margin-top:16px; line-height:42px; }

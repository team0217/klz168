.user-header .user-header-hint{ margin:36px 0; font-size:30px; color:#5d5d5d; padding-left:18px; margin-left:20px; border-left:solid 1px #c5c5c5; }
.user-header .user-nav{ color:#666666; margin-top:55px;  height:30px; line-height:30px; font-size: 12px; }

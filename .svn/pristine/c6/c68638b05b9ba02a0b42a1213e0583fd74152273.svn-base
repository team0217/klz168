#login-content{ height:500px; }
#login-content .wrap-and{ padding:0; height:500px; }
#login-content .user-bg-img{ width:784px; height:100%; background:#fff; position: absolute; left:0; }
#login-content .user-bg-img img{ height:460px; width:100%; margin:20px 0; }
#login-content .login-wrap{ color:#5d5d5d; height:100%; position: absolute; right:0; background:#fff; padding:30px 33px; width:360px; height:438px; border:solid 1px #dcdcdc;  }
#login-content .top{ height:58px; line-height:58px; border-bottom:solid 1px #dcdcdc; }
#login-content .top li{ width:50%; float:left; text-align: center; }
#login-content .top li a{ display:block; height:100%; }
#login-content .top li .ico{
	display:inline-block;width:22px; height:28px; vertical-align: middle; margin-right:12px; margin-top:-5px;
	_display:block; _float:left; _margin-left:20px; _margin-top:15px;
}
#login-content .top li .ico.user{ background:url(../images/user-l-icon.png) no-repeat; }
#login-content .top li .ico.shops{ background:url(../images/shops-l-icon.png) no-repeat; }
#login-content .top li.active a{ color:#FF6C00; border-bottom:solid 1px #ff6c00; }
#login-content .top li.active .ico.user{ background:url(../images/user-l-icon_active.png) no-repeat; }
#login-content .top li.active .ico.shops{ background:url(../images/shops-l-icon_active.png) no-repeat; }
#login-content .l-u-list{ font-size:14px; margin-top:18px; }
#login-content .l-u-list .lu-box li.t{ height:46px; line-height:46px; }
#login-content .l-u-list .lu-box li.i{ height:42px; line-height:42px; }
#login-content .l-u-list .lu-box li input.s{ padding:0 8px; height:40px; width:340px; border:solid 1px #d7d7d7; }
#login-content .l-u-list .lu-box li.k{ height:56px; line-height:56px; padding-top:4px; }
#login-content .l-u-list .lu-box li.b{ height:62px; line-height:62px; }
#login-content .l-u-list .lu-box li.b input{ font-size:30px; cursor:pointer; height:100%; color:#fff; border:none; background:#FF6C00; width:100%; }
#login-content .l-u-list .lu-box li.ic{ margin-top:30px; }
#login-content .l-u-list .lu-box li .qq-icon{ background:url(../images/index_qq_icon_o.png) no-repeat; padding-left:20px; height:18px; line-height:18px; display:block; _width:50%; }

/* 注册 */
#zc-content .zc-top{ height:38px; width:100%; border-bottom:solid 2px #FF6C00; position:relative; }
#zc-content .zc-top .tab-title{ font-size:16px; overflow: hidden; width:340px; left:50%; margin-left:-170px; height:38px; line-height:38px; position:absolute; }
#zc-content .zc-top .tab-title a{ width:160px; height:100%; display:block; text-align: center; border:solid 1px #c3c3c3; height:38px; margin:0; }
#zc-content .zc-top .tab-title a.active{ border-color:#FF6C00; background:#FF6C00; color:#fff; }
#zc-content .zc-top .zc-title{ font-size:12px; bottom:8px; position:absolute; right:0; }
#zc-content .zc-top .zc-title a{ color:#FF6C00; margin:0 4px; }
#zc-content .zc-lc{ font-size:12px; position:relative; width:596px; margin:58px auto 0; height:44px; }
#zc-content .zc-lc li{ position:absolute; top:0; padding-bottom:22px; text-align: center; height:22px; width:102px; }
#zc-content .zc-lc li.linear{ height:1px; width:100%; padding:0; top:100%; margin-top:-11px; z-index:-1;  background:#dcdcdc; }
#zc-content .zc-lc b{ position:absolute; bottom:0; display: block; width:22px; height:22px; background:url(../images/zc-lc-off.png) no-repeat center center; left:50%; margin-left:-11px; }
#zc-content .zc-lc li.active{ color:#FF6C00; }
#zc-content .zc-lc li.active b{ background:url(../images/zc-lc-on.png) no-repeat center center; }
#zc-content .zc-lc li.li1{ left:0; margin-left:-61px; }
#zc-content .zc-lc li.li2{ left:50%; margin-left:-61px; }
#zc-content .zc-lc li.li3{ right:0; margin-right:-61px; }

#zc-content .user_l_c{ font-size:14px; padding:20px 0; background:#fff; margin:48px 0 50px; }
#zc-content .user_l_c .u_tab{ width:580px; margin:0 auto; height:58px; }
